{
  "name": "dhbw-projekt-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pwa": "ng build --prod && sw-precache --root=public --config=precache-config.js",
    "test": "jest --browser",
    "test:ci": "jest --browser --runInBand",
    "test:coverage": "jest --browser --runInBand --coverage",
    "docs": "./node_modules/.bin/compodoc -d ./docs/ -p ./tsconfig.json -n \"Eventpicking - Dokumentation\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ngx-translate/core": "^9.0.1",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/stripe-checkout": "^1.0.1",
    "angularfire2": "5.0.0-rc.4",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.2",
    "easy-pie-chart": "^2.1.7",
    "firebase": "4.6.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "ng-mdb-pro": "git+https://oauth2:w67qvcj6w1FbSFmHZpjX@git.mdbootstrap.com/mdb/angular/ng-pro.git",
    "ng2-logger": "^1.0.3",
    "rxjs": "^5.5.5",
    "screenfull": "^3.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^21.1.8",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^21.2.1",
    "jest-preset-angular": "^4.0.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.conf.ts",
    "coverageDirectory": "./coverage/",
    "setupFiles": [
      "<rootDir>/jest-mocks/shimSetup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@ngrx|ng-mdb-pro))"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/environments/*{ts}",
      "!**/jest-mocks/*{ts}",
      "!**/src/*{ts}",
      "!**/*d.{ts}",
      "!**/*interface.{ts}",
      "!**/*module.{ts}",
      "!**/*main.{ts}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
