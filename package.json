  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.conf.ts",
    "coverageDirectory": "./coverage/",
    "setupFiles": [
      "<rootDir>/jest-mocks/shimSetup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@ngrx|ng-mdb-pro))"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/environments/*{ts}",
      "!**/jest-mocks/*{ts}",
      "!**/src/*{ts}",
      "!**/*d.{ts}",
      "!**/*interface.{ts}",
      "!**/*module.{ts}",
      "!**/*main.{ts}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
